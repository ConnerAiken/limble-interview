{
  "name": "limble-labor-metrics-api",
  "version": "1.0.0",
  "description": "A simple express API for limble labor metrics",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "mocha ./src/tests/**/*.test.js",
    "tests:all": "mocha ./src/tests/**/*.test.js",
    "tests:unit": "mocha ./src/tests/unit/*.test.js",
    "tests:integration": "mocha ./src/tests/integration/*.test.js",
    "test:watch": "mocha --watch ./src/tests/*.test.js",
    "docs": "apidoc -i ./src -o ./docs"
