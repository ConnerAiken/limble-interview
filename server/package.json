  "author": "Conner Aiken",
  "type": "module",
  "dependencies": {
    "cache-express": "^1.0.2",
    "compression": "^1.7.4",
    "express": "^4.18.3",
    "mariadb": "^3.2.3",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "apidoc": "^1.2.0",
    "chai": "^5.1.1",
    "mocha": "^10.7.0",
    "supertest": "^7.0.0"
  }
}
